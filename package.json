    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.3",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^16.2.3",
    "@angular/compiler": "^16.2.3",
    "@angular/core": "^16.2.3",
    "@angular/forms": "^16.2.3",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^16.2.3",
    "@angular/platform-browser-dynamic": "^16.2.3",
    "@angular/router": "^16.2.3",
    "@ngneat/lib": "^5.0.0",
    "rxjs": "^6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.1",
    "@angular/cli": "^16.2.1",
    "@angular/compiler-cli": "^16.2.3",
    "@angular/language-service": "^16.2.3",
    "@commitlint/cli": "17.6.6",
    "@commitlint/config-angular": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@ngneat/spectator": "^11.3.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^18.16.19",
    "all-contributors-cli": "^6.8.1",
    "codelyzer": "^6.0.0",
    "cross-env": "^7.0.3",
    "git-cz": "^4.9.0",
    "husky": "^8.0.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.20",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^13.2.3",
    "ng-packagr": "^16.2.2",
    "prettier": "^2.8.8",
    "protractor": "~7.0.0",
    "standard-version": "^9.5.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~5.1.6"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,ts,html,component.html}": [
      "prettier --write"
    ]
  }
}
